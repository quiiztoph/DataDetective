{
  "name": "Data Detective",
  "version": "1.0",
  "description": "Build an Extension!",
  "permissions": ["contextMenus","activeTab", "declarativeContent", "storage"],
  "background": {
    "scripts": ["background.js", "locales.js"],
    "persistent": false
  },
  "browser_action": {
  "default_title": "Access to NASA Datasets for Earth Science",
  "default_popup": "popup.html",
  "default_icon": {
    "16":"images/satellite.png",
    "128":"images/satellite.png"
  }
},

"icons": {
  "16": "images/satellite.png",
  "128":"images/satellite.png"
},

 "options_page": "options.html",

  "manifest_version": 2
}
